{
  "name": "@tilli-pro/prettier-config",
  "version": "0.1.1",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": "./index.js"
  },
  "scripts": {
    "clean": "git clean -xdf .cache .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "catalog:",
    "prettier": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:"
  },
  "devDependencies": {
    "@tilli-pro/tsconfig": "workspace:*",
    "typescript": "catalog:"
  },
  "prettier": "@tilli-pro/prettier-config"
}
