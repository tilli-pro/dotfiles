{
	"$schema": "https://biomejs.dev/schemas/2.2.5/schema.json",
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true
	},
	"files": {
		"ignoreUnknown": false,
		"maxSize": 16777216
	},
	"formatter": {
		"enabled": true,
		"formatWithErrors": false,
		"indentStyle": "space",
		"indentWidth": 2,
		"lineEnding": "lf",
		"lineWidth": 80,
		"attributePosition": "auto",
		"bracketSameLine": false,
		"bracketSpacing": true,
		"expand": "auto",
		"useEditorconfig": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": false,
			"complexity": {
				"noVoid": "off",

				"noBannedTypes": "warn",
				"noUselessContinue": "warn",
				"noUselessTernary": "warn",

				"noAdjacentSpacesInRegex": "error",
				"noArguments": "error",
				"noExtraBooleanCast": "error",
				"noUselessCatch": "error",
				"noUselessEscapeInRegex": "error",
				"noUselessTypeConstraint": "error",
				"noImportantStyles": "warn",
				"noUselessRename": "error",
				"useArrowFunction": "warn",
				"useRegexLiterals": "warn",
				"useWhile": "error",
				"noEmptyTypeParameters": "error",
				"noThisInStatic": "error",
				"noStaticOnlyClass": "error",
				"noUselessThisAlias": "warn",
				"useSimplifiedLogicExpression": "warn",
				"useDateNow": "warn",
				"noImplicitCoercions": "off"
			},
			"correctness": {
				"useYield": "warn",
				"useExhaustiveDependencies": "warn",
				"useHookAtTopLevel": "warn",
				"useParseIntRadix": "warn",
				"noUnusedImports": "warn",
				"noUnusedLabels": "error",
				"noUnusedPrivateClassMembers": "error",
				"noUnusedVariables": "error",
				"noConstAssign": "error",
				"noConstantCondition": "error",
				"noEmptyCharacterClassInRegex": "error",
				"noEmptyPattern": "error",
				"noGlobalObjectCalls": "error",
				"noInvalidBuiltinInstantiation": "error",
				"noInvalidConstructorSuper": "error",
				"noNonoctalDecimalEscape": "error",
				"noPrecisionLoss": "error",
				"noSelfAssign": "error",
				"noSetterReturn": "error",
				"noSwitchDeclarations": "error",
				"noUndeclaredVariables": {
					"level": "error",
					"options": { "checkTypes": true }
				},
				"noUnreachable": "error",
				"noUnreachableSuper": "error",
				"noUnsafeFinally": "error",
				"noUnsafeOptionalChaining": "error",
				"useIsNan": "error",
				"useJsxKeyInIterable": "error",
				"useSingleJsDocAsterisk": "error",
				"useValidForDirection": "error",
				"useValidTypeof": "error",
				"noReactPropAssignments": "error",
				"noConstructorReturn": "error",
				"noInvalidGridAreas": "warn",
				"noInvalidUseBeforeDeclaration": "error",
				"noVoidTypeReturn": "error"
			},
			"style": {
				"noImplicitBoolean": "warn",
				"noInferrableTypes": "error",
				"noNamespace": "error",
				"noUnusedTemplateLiteral": "warn",
				"noYodaExpression": "error",
				"useConsistentArrayType": {
					"level": "error",
					"options": {
						"syntax": "shorthand"
					}
				},
				"useArrayLiterals": "error",
				"useAsConstAssertion": "error",
				"useConsistentCurlyBraces": "warn",
				"useConst": "warn",
				"useBlockStatements": "off",
				"useDeprecatedReason": "error",
				"useExportType": "warn",
				"useFragmentSyntax": "error",
				"useImportType": "error",
				"useNodejsImportProtocol": "error",
				"useShorthandAssign": "warn",
				"useTemplate": "error",
				"useThrowNewError": "warn",
				"useObjectSpread": "error",
				"useGroupedAccessorPairs": "error",
				"useNamingConvention": "warn",
				"useReadonlyClassProperties": "warn",
				"noDefaultExport": "warn",
				"noDoneCallback": "warn",
				"noNonNullAssertion": "warn",
				"useThrowOnlyError": "warn",
				"noSubstr": "warn",
				"useDefaultSwitchClause": "warn"
			},
			"suspicious": {
				"noArrayIndexKey": "warn",
				"noAsyncPromiseExecutor": "error",
				"noCatchAssign": "error",
				"noClassAssign": "error",
				"noCommentText": "error",
				"noCompareNegZero": "error",
				"noConstantBinaryExpressions": "warn",
				"noControlCharactersInRegex": "error",
				"noDoubleEquals": "error",
				"noDebugger": "error",
				"noDuplicateCase": "error",
				"noDuplicateClassMembers": "error",
				"noDuplicateElseIf": "error",
				"noDuplicateJsxProps": "error",
				"noDuplicateObjectKeys": "warn",
				"noDuplicateParameters": "error",
				"noEmptyBlockStatements": "error",
				"noExplicitAny": "error",
				"noExtraNonNullAssertion": "error",
				"noFallthroughSwitchClause": "error",
				"noFunctionAssign": "error",
				"noGlobalAssign": "error",
				"noImplicitAnyLet": "warn",
				"noImportAssign": "error",
				"noIrregularWhitespace": "error",
				"noMisleadingCharacterClass": "error",
				"noMisleadingInstantiator": "error",
				"noPrototypeBuiltins": "error",
				"noRedeclare": "error",
				"noShadowRestrictedNames": "error",
				"noSparseArray": "error",
				"noSuspiciousSemicolonInJsx": "warn",
				"noUnsafeDeclarationMerging": "error",
				"noUnsafeNegation": "error",
				"noVar": "error",
				"noWith": "error",
				"useGetterReturn": "error",
				"useIsArray": "warn",
				"noUnassignedVariables": "warn",
				"noGlobalIsNan": "error",
				"noGlobalIsFinite": "error",
				"useErrorMessage": "warn"
			},
			"a11y": {
				"recommended": true,
				"noRedundantAlt": "error",
				"noStaticElementInteractions": "error",
				"useAltText": "error",
				"useValidLang": "error"
			},
			"performance": {
				"noAwaitInLoops": "warn",
				"useTopLevelRegex": "info",
				"noAccumulatingSpread": "warn",
				"noDelete": "warn"
			},
			"security": {
				"noBlankTarget": "error",
				"noDangerouslySetInnerHtmlWithChildren": "error",
				"noGlobalEval": "error",
				"noDangerouslySetInnerHtml": "warn"
			},
			"nursery": {
				"noFloatingPromises": "warn",
				"useSortedClasses": "warn",
				"noUselessUndefined": "warn",
				"useExhaustiveSwitchCases": "warn",
				"noImportCycles": "error",
				"noDeprecatedImports": "warn",
				"noNextAsyncClientComponent": "error",
				"noSecrets": "warn",
				"noShadow": "warn",
				"noMisusedPromises": "warn",
				"noUnnecessaryConditions": "warn",
				"noUnresolvedImports": "warn",
				"useAnchorHref": "warn",
				"useImageSize": "warn",
				"noUselessCatchBinding": "warn",
				"useReactFunctionComponents": "warn"
			}
		}
	},
	"javascript": {
		"formatter": {
			"jsxQuoteStyle": "double",
			"quoteProperties": "asNeeded",
			"trailingCommas": "all",
			"semicolons": "asNeeded",
			"arrowParentheses": "always",
			"bracketSameLine": false,
			"quoteStyle": "double",
			"attributePosition": "auto",
			"bracketSpacing": true,
			"indentStyle": "space",
			"indentWidth": 2,
			"lineEnding": "lf",
			"lineWidth": 80,
			"enabled": true,
			"expand": "auto",
			"operatorLinebreak": "after"
		},
		"globals": ["exports", "React", "JSX"],
		"linter": {
			"enabled": true
		},
		"parser": {
			"jsxEverywhere": false,
			"unsafeParameterDecoratorsEnabled": true,
			"gritMetavariables": true
		},
		"assist": {
			"enabled": true
		},
		"jsxRuntime": "transparent"
	},
	"html": {
		"parser": {
			"interpolation": true
		},
		"formatter": {
			"enabled": true,
			"selfCloseVoidElements": "always",
			"lineEnding": "lf",
			"lineWidth": 80,
			"indentStyle": "space",
			"indentWidth": 2,
			"whitespaceSensitivity": "css"
		}
	},
	"css": {
		"formatter": {
			"enabled": true,
			"lineWidth": 80,
			"indentStyle": "space",
			"indentWidth": 2,
			"lineEnding": "lf",
			"quoteStyle": "double"
		},
		"linter": {
			"enabled": true
		},
		"assist": {
			"enabled": true
		},
		"parser": {
			"cssModules": true
		},
		"globals": []
	},
	"json": {
		"parser": {
			"allowComments": true,
			"allowTrailingCommas": false
		},
		"assist": {
			"enabled": true
		},
		"linter": {
			"enabled": true
		},
		"formatter": {
			"enabled": true,
			"lineWidth": 80,
			"indentStyle": "space",
			"indentWidth": 2,
			"lineEnding": "lf"
		}
	},
	"assist": {
		"enabled": true,
		"actions": {
			"source": {
				"organizeImports": {
					"level": "on",
					"options": {
						"identifierOrder": "natural",
						"groups": [
							":URL:",
							":NODE:",
							":PACKAGE:",
							":ALIAS:",
							{ "type": false },
							":PATH:"
						]
					}
				},
				"useSortedAttributes": "on",
				"useSortedProperties": "on",
				"useSortedKeys": "on"
			}
		}
	},
	"plugins": [],
	"overrides": [
		{
			"includes": [
				"**",
				"!.*.js",
				"!node_modules/",
				"!dist/",
				"!build/",
				"!out/"
			]
		}
	]
}
